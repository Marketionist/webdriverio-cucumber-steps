{
  "name": "webdriverio-cucumber-steps",
  "version": "0.6.0",
  "description": "Cucumber steps (step definitions) written with WebdriverIO for end-to-end (e2e) tests",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint *.js tests/**/*.feature",
    "test": "./node_modules/.bin/wdio tests/wdio.conf.js",
    "test:ci": "./node_modules/.bin/wdio tests/wdio.ci.conf.js",
    "patch": "npm version patch -m \"Bump up package version to %s\" && git push && git push origin --tags && npm publish",
    "minor": "npm version minor -m \"Bump up package version to %s\" && git push && git push origin --tags && npm publish",
    "major": "npm version major -m \"Bump up package version to %s\" && git push && git push origin --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Marketionist/webdriverio-cucumber-steps"
  },
  "keywords": [
    "webdriverio cucumber",
    "webdriverio-cucumber",
    "webdriverio cucumber steps",
    "webdriverio-cucumber-steps",
    "cucumber steps",
    "cucumber step definitions",
    "webdriverio plugin",
    "automated tests",
    "e2e tests",
    "end-to-end testing",
    "acceptance testing",
    "browser testing",
    "webdriverio",
    "cucumber",
    "cucumberjs",
    "cucumber-js",
    "gherkin",
    "bdd"
  ],
  "author": "Dmytro Shpakovskyi",
  "license": "MIT",
  "dependencies": {
    "js-automation-tools": "^1.0.5"
  },
  "peerDependencies": {
    "@wdio/cli": ">=6.0.8",
    "@wdio/cucumber-framework": ">=6.0.8",
    "@wdio/local-runner": ">=6.0.8",
    "@wdio/selenium-standalone-service": ">=6.0.8",
    "@wdio/spec-reporter": ">=6.0.8",
    "chromedriver": ">=88.0.0",
    "wdio-chromedriver-service": ">=6.0.3"
  },
  "devDependencies": {
    "@wdio/cli": "^7.0.7",
    "@wdio/cucumber-framework": "^7.0.7",
    "@wdio/local-runner": "^7.0.7",
    "@wdio/selenium-standalone-service": "^7.0.7",
    "@wdio/spec-reporter": "^7.0.7",
    "chromedriver": "^88.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-cucumber": "^1.4.0",
    "node-testing-server": "^1.5.1",
    "wdio-chromedriver-service": "^7.0.0"
  }
}
